<?xml version="1.0"?>
<!DOCTYPE rdf:RDF [
   <!ENTITY soupa  "http://pervasive.semanticweb.org/ont/2004/06/">
   <!ENTITY rdf    "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
   <!ENTITY rdfs   "http://www.w3.org/2000/01/rdf-schema#">
   <!ENTITY owl    "http://www.w3.org/2002/07/owl#"> 
   <!ENTITY xsd    "http://www.w3.org/2001/XMLSchema#"> 
   <!ENTITY bdi    "http://pervasive.semanticweb.org/ont/2004/06/bdi#">
   <!ENTITY agt    "http://pervasive.semanticweb.org/ont/2004/06/agent#">
   ]>
